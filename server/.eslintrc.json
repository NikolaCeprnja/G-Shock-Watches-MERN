{
  "env": {
    "node": true,
    "es6": true,
    "commonjs": true
  },
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "extends": [
    "airbnb-base",
    "plugin:node/recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-unused-vars": "warn",
    "func-names": "off",
    "object-shorthand": "off",
    "consistent-return": "off",
    "default-case": "off"
  }
}